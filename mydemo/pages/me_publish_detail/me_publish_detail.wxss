  width: 70%;
  position: center;
  font-size: 35rpx;
  height: 85rpx;
}
.hide {
  display: flex;
  justify-content: center;
  margin-top: 80rpx;
  background-color: rgba(140, 195, 231, 0.5); 
  width: 70%;
  position: center;
  font-size: 35rpx;
  height: 85rpx;
  background: #ccc;
}
.hide image, .show image {
